{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BotConfiguration": {
    "BotName": "Arty",
    "AadAppId": "YOUR_BOT_APP_ID",
    "AadAppSecret": "YOUR_BOT_APP_SECRET",
    "ServiceDnsName": "YOUR_NGROK_DOMAIN",
    "ServiceCname": "YOUR_NGROK_DOMAIN",
    "CertificateThumbprint": "YOUR_CERTIFICATE_THUMBPRINT",
    "InstancePublicPort": 8445,
    "InstanceInternalPort": 8445,
    "CallSignalingPort": 9441,
    "PlaceCallEndpointUrl": "https://graph.microsoft.com/v1.0",
    "GraphApiBaseUrl": "https://graph.microsoft.com/v1.0"
  },
  "AudioSettings": {
    "OutputFolder": "AudioCapture",
    "SampleRate": 16000,
    "BitsPerSample": 16,
    "Channels": 1,
    "CaptureUnmixedAudio": true
  },
  "PythonBackend": {
    "BaseUrl": "http://localhost:8000",
    "TranscriptionWebhookPath": "/api/arty/transcription",
    "StatusWebhookPath": "/api/arty/status"
  }
}

